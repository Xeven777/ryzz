generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model Message {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  content   String
  userId    String
  createdAt DateTime @default(now())
  seen      Boolean  @default(false)

  @@map("messages")
}
